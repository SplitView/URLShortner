{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MongoSettings": {
    "CustomURLCollectionName": "custom_url",
    "RedirectionLogCollectionName": "redirection_log",
    "ConnectionString": "mongodb://urlshortner.db:27017",
    "DatabaseName": "BookstoreDb"
  },
  "AppConfig": {
    "ExpiryTimeInSeconds": 64000
  },
  "Redis": {
    "ConnectionString": "urlshortner.redis:6379"
  },
  "RabbitMq": {
    "ConnectionString": "amqp://guest:guest@urlshortner.rabbitmq:5672"
  }
}
